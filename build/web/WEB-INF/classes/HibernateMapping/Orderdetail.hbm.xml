<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC '-//Hibernate/Hibernate Mapping DTD 3.0//EN' 'http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd'>
<hibernate-mapping> 
    <class name="EntityCollection.OrderdetailEntity" table="orderdetail">
        <id name="orderdetailid" type="integer" column="odid">
            <generator class="native" />
        </id>
        <property name="productid" type="integer" column="pid" />
        <property name="productname" type="string" column="pname" />
        <property name="quantity" type="integer" column="qty" />
        <property name="price" type="integer" column="price" />
        <property name="orderid" type="integer" column=" oid" />
       
                  
    </class>
</hibernate-mapping>

